{
  "testnet": true,
  "timeframe": "1m",
  "symbol": "XBTUSD",
  "name": "mm",

  "modules": [
    "core/adapter",
    "core/archivist",
    "core/chandler",
    "core/analyst/bb",
    "core/brain/mm",
    "core/trader",
    "core/executor/mm"
  ],

  "backbone": {
    "chain": [
      ["SocketConnected", "DownloadHistory"],
      ["HistoryDownloaded", "WatchMarket"]
    ],
    "emit": "ConnectSocket",
    "verbose": true,
    "ignore": ["TradeOpened", "QuoteOpened", "MarginUpdated", "PositionUpdated", "OrderUpdated"]
  },

  "archivist": {
    "history": "20"
  },

  "chandler": {
    "offset": -2000
  },

  "analyst": {
    "history": "20",
    "bb": {
      "periods": 20,
      "mult": 0.5
    }
  },

  "brain": {
    "skip": 0
  },

  "trader": {
    "positions": 1,
    "size": 0.02
  },

  "executor": {
    "speed": 200,
    "expiration": 600000,
    "entry": 0.005,
    "sl": 0.01,
    "bounce": 0.5,
    "target": "bb_ma"
  }
}
